train_net: "prototxts/MDNet/pretrain.prototxt"
base_lr: 0.0001
lr_policy: "fixed"
gamma: 0.1
display: 20
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "MDNet"
